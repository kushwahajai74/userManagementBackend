<%- include('./partials/header') %>
  <div class = "container mt-5">
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
          <th scope="col">Email</th>
          <th scope="col">First Name</th>
          <th scope="col">Last Name</th>
          <th scope="col">Phone</th>
          <th scope="col">Aadhar</th>
          <th scope="col">Address</th>
          <th scope="col">city</th>
          <th scope="col">pincode</th>
          <th scope="col">institute</th>
          <th scope="col">description</th>
          <th scope="col" >role</th>
          <th scope="col" >StartsAt</th>
          <th scope="col" >endsAt</th>
          <th scope="col" >Managed By</th>
        </tr>
    </thead>
    <tbody>
      <% employees.forEach(employee => { %>
          <tr class="align-middle">
            <td><a href="/user/<%= employee._id %>"><%= employee.email %></a></td>
            <td><%= employee.fname %></td>
            <td><%= employee.lname %></td>
            <td><%= employee.contact %></td>
            <td><%= employee.aadhar %></td>
            <td><%= employee.address %></td>
            <td><%= employee.city %></td>
            <td><%= employee.pincode %></td>
            <td><%= employee.institute %></td>
            <td><%= employee.description %></td>
            <td><%= employee.role %></td>
            <td><%= employee.startsAt %></td>
            <td><%= employee.endsAt %></td>
            <td><%= employee.managedBy %></td>
          </tr>
          <% }) %>
      </tr>
    </tbody>
  </table>
  </div>